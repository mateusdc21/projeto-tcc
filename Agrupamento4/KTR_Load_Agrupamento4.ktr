    <sql>&#x2f;&#x2a; AGRUPAMENTO 4, ACRESCENTANDO A DIMENS&#xc3;O DE ESTADO CIVIL &#x2a;&#x2f;&#xd;&#xa;SELECT CAST&#x28;&#x28;CAST&#x28;v4752 AS integer&#x29; &#x2b; 400&#x29; AS VARCHAR&#x28;3&#x29;&#x29; as v4752, --idade&#xd;&#xa;		v0401, --sexo&#xd;&#xa;		v0408, --ra&#xe7;a&#x2f;cor&#xd;&#xa;		CASE WHEN v0438 &#x3d; &#x27;3&#x27; THEN &#x27;2&#x27;&#xd;&#xa;		     ELSE v0438&#xd;&#xa;		END as v0438, --estado civil&#xd;&#xa;		LEFT&#x28;v0103,6&#x29; as v0103, --municipio&#xd;&#xa;		sum&#x28;CAST&#x28;LEFT&#x28;p001, 3&#x29; &#x7c;&#x7c; &#x27;.&#x27; &#x7c;&#x7c; RIGHT&#x28;p001, 8&#x29; AS numeric&#x29;&#x29; as qtd,&#xd;&#xa;		COUNT&#x28;1&#x29; as num_individuos&#xd;&#xa;FROM censo2000&#xd;&#xa;GROUP BY v4752, v0401, v0408, v0438, v0103&#xd;&#xa;ORDER BY v4752 DESC, v0401 DESC, v0408 DESC, v0438 DESC, v0103 DESC, qtd DESC</sql>
